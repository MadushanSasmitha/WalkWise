<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>en</string>
	<key>CFBundleDisplayName</key>
	<string>WalkWise</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleName</key>
	<string>WalkWise</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>

	<!-- iPhone only, portrait preferred -->
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
	</array>

	<!-- HealthKit read permissions message -->
	<key>NSHealthShareUsageDescription</key>
	<string>WalkWise reads your steps, walking distance, and exercise minutes to show progress and insights.</string>

	<!-- Photos usage for profile image picker -->
	<key>NSPhotoLibraryUsageDescription</key>
	<string>WalkWise needs access to your photos to let you choose a profile picture.</string>
</dict>
</plist>
